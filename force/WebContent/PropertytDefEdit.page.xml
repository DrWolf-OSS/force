<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/PropertytDefList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{propertytDefHome.wire}"/>

   <param name="propertytDefFrom"/>
   <param name="propertytDefId" value="#{propertytDefHome.propertytDefId}"/>


   <navigation from-action="#{propertytDefHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/PropertytDef.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{propertytDefHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/PropertytDef.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{propertytDefHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/PropertytDefList.xhtml"/>
      </rule>
   </navigation>

</page>
