<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/PropertyInstList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{propertyInstHome.wire}"/>

   <param name="propertyInstFrom"/>
   <param name="propertyInstId" value="#{propertyInstHome.propertyInstId}"/>
   <param name="propertytDefFrom"/>
   <param name="propertytDefId" value="#{propertytDefHome.propertytDefId}"/>



   <navigation from-action="#{propertyInstHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/PropertyInst.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{propertyInstHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/PropertyInst.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{propertyInstHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/PropertyInstList.xhtml"/>
      </rule>
   </navigation>

</page>
